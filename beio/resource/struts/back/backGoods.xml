<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="backGoods" extends="beio-default" namespace="/backGoods">
	
		<!-- 查询品牌 -->
		<action name="pageBrand" class="backGoodsAction" method="pageBrand">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<!-- 保存品牌 -->
		<action name="saveBrand" class="backGoodsAction" method="saveBrand">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<!-- 删除品牌 -->
		<action name="delBrand" class="backGoodsAction" method="delBrand">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<!-- 向上品牌 -->
		<action name="upBrand" class="backGoodsAction" method="upBrand">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<!-- 向下品牌 -->
		<action name="downBrand" class="backGoodsAction" method="downBrand">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<!-- 查询分类 -->
		<action name="pageClassify" class="backGoodsAction" method="pageClassify">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<!-- 保存分类 -->
		<action name="saveClassify" class="backGoodsAction" method="saveClassify">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<!-- 删除分类 -->
		<action name="delClassify" class="backGoodsAction" method="delClassify">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<!-- 向上分类 -->
		<action name="upClassify" class="backGoodsAction" method="upClassify">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<!-- 向下分类 -->
		<action name="downClassify" class="backGoodsAction" method="downClassify">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
	
	
	
	
		<action name="pageGoods" class="backGoodsAction" method="pageGoods">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="getAllClassify" class="backGoodsAction" method="getAllClassify">
			<result name="json" type="json">
				<param name="root">jsonArray</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="getAllBrand" class="backGoodsAction" method="getAllBrand">
			<result name="json" type="json">
				<param name="root">jsonArray</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="saveGoods" class="backGoodsAction" method="saveGoods">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="delGoods" class="backGoodsAction" method="delGoods">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="queryGoodsImg" class="backGoodsAction" method="queryGoodsImg">
			<result name="json" type="json">
				<param name="root">jsonArray</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="saveGoodsImg" class="backGoodsAction" method="saveGoodsImg">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="delGoodsImg" class="backGoodsAction" method="delGoodsImg">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="upGoodsImg" class="backGoodsAction" method="upGoodsImg">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="downGoodsImg" class="backGoodsAction" method="downGoodsImg">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		
		
		
		
		<action name="pageOrder" class="backGoodsAction" method="pageOrder">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="sendGoods" class="backGoodsAction" method="sendGoods">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
		<action name="cancelOrder" class="backGoodsAction" method="cancelOrder">
			<result name="json" type="json">
				<param name="root">backRoot</param>
			</result>
			<interceptor-ref name="backIdentityStack"/>
		</action>
		
	</package>
</struts>